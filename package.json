{
  "name": "babel-plugin",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run transform && npm run minify && npm run flatten && npm run traverse && npm run server",
    "transform": "babel input.js -o output.js",
    "minify": "uglifyjs output.js -o output.min.js --compress --source-map 'url=inline'",
    "flatten": "sorcery -d -i output.min.js",
    "traverse": "node lib/traverse.js",
    "server": "node lib/server.js"
  },
  "author": "RetroAstro",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/parser": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/traverse": "^7.5.5",
    "sorcery": "^0.10.0",
    "uglify-js": "^3.6.0",
    "ws": "^7.1.2"
  }
}
